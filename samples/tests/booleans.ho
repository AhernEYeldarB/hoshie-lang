//  Not  ---
utest(true, !false);
utest(!true, false);
utest(!!!!!true, !!!!!!false, "Lots of nots");

//  Equality  ---
utest(false == false, true);
utest(false == true, false);
utest(true == false, false);
utest(true == true, true);
utest(false != false, false);
utest(false != true, true);
utest(true != false, true);
utest(true != true, false);
utest(10 == 10, true);
utest(10 == 11, false);
utest(11 == 10, false);
utest(11 == 11, true);
utest(10 != 10, false);
utest(10 != 11, true);
utest(11 != 10, true);
utest(11 != 11, false);
utest("10" == "10", true);
utest("10" == "11", false);
utest("11" == "10", false);
utest("11" == "11", true);
utest("10" != "10", false);
utest("10" != "11", true);
utest("11" != "10", true);
utest("11" != "11", false);

//  Comparison  ---
utest(false > false, false);
utest(false > true, false);
utest(true > false, true);
utest(true > true, false);
utest(false >= false, true);
utest(false >= true, false);
utest(true >= false, true);
utest(true >= true, true);
utest(false < false, false);
utest(false < true, true);
utest(true < false, false);
utest(true < true, false);
utest(false <= false, true);
utest(false <= true, true);
utest(true <= false, false);
utest(true <= true, true);
utest(10 > 10, false);
utest(10 > 11, false);
utest(11 > 10, true);
utest(11 > 11, false);
utest(10 >= 10, true);
utest(10 >= 11, false);
utest(11 >= 10, true);
utest(11 >= 11, true);
utest(10 < 10, false);
utest(10 < 11, true);
utest(11 < 10, false);
utest(11 < 11, false);
utest(10 <= 10, true);
utest(10 <= 11, true);
utest(11 <= 10, false);
utest(11 <= 11, true);
utest("a" > "a", false);
utest("a" > "b", false);
utest("b" > "a", true);
utest("b" > "b", false);
utest("a" >= "a", true);
utest("a" >= "b", false);
utest("b" >= "a", true);
utest("b" >= "b", true);
utest("a" < "a", false);
utest("a" < "b", true);
utest("b" < "a", false);
utest("b" < "b", false);
utest("a" <= "a", true);
utest("a" <= "b", true);
utest("b" <= "a", false);
utest("b" <= "b", true);

//  Logic  --
utest(false && false, false);
utest(false && true, false);
utest(true && false, false);
utest(true && true, true);
utest(false || false, false);
utest(false || true, true);
utest(true || false, true);
utest(true || false, true);
